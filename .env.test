# Test Environment Variables
# متغيرات البيئة للاختبار

# Application Environment
NODE_ENV=test
VITE_APP_ENV=test
VITE_APP_URL=http://localhost:5173
VITE_API_URL=http://localhost:3001/api

# Security Keys for Testing
VITE_ENCRYPTION_KEY=test_32_character_encryption_key_123
VITE_CSRF_SECRET=test_csrf_secret_key_for_testing

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_AUTH=true
VITE_ENABLE_NEW_BACKEND=true
VITE_USE_NEW_AUTH=true

# Rate Limiting (Permissive for testing)
VITE_RATE_LIMIT_REQUESTS=1000
VITE_RATE_LIMIT_WINDOW=900000

# Session Configuration
VITE_SESSION_TIMEOUT=3600000
VITE_REMEMBER_ME_DURATION=86400000
