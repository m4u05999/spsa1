# Environment Variables for SPSA Website

# API Configuration
VITE_APP_API_URL=https://api.example.com
VITE_APP_API_KEY=your_api_key

# Authentication
VITE_APP_AUTH_DOMAIN=your_auth_domain
VITE_APP_CLIENT_ID=your_client_id

# Supabase Configuration
VITE_APP_SUPABASE_URL=your_supabase_url
VITE_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# Application Settings
VITE_APP_ENV=development
VITE_APP_NAME="الجمعية السعودية للعلوم السياسية"
VITE_APP_URL=http://localhost:3000

# Payment Gateway (if implemented)
VITE_APP_PAYMENT_API_KEY=your_payment_api_key
VITE_APP_PAYMENT_SECRET=your_payment_secret
