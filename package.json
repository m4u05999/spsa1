{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint ./src --quiet",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:phase1": "node scripts/runPhase1Tests.js",
    "test:integration": "vitest run src/tests/integration/",
    "test:compatibility": "vitest run src/tests/moduleCompatibility.test.js",
    "test:supabase-fix": "vitest run src/tests/supabaseFix.test.js",
    "test:error-resolution": "vitest run src/tests/errorResolution.test.js",
    "test:environment": "vitest run src/tests/environmentConfig.test.js",
    "test:syntax-fix": "vitest run src/tests/syntaxFix.test.js",
    "test:advanced-search": "vitest run src/tests/advancedSearch.test.jsx",
    "test:file-upload": "vitest run src/tests/fileUpload.test.jsx",
    "test:user-management": "vitest run src/tests/userManagement.test.js",
    "test:local-storage": "vitest run src/tests/userManagement.test.js --grep=\"نظام التخزين المحلي\"",
    "verify:fallback": "node scripts/verifyFallbackMechanisms.js",
    "check:env": "node scripts/checkEnvironment.js",
    "fix:env": "node scripts/fixEnvironmentFiles.js"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^6.4.12",
    "@mui/material": "^6.0.2",
    "@supabase/supabase-js": "^2.47.12",
    "@types/prop-types": "^15.7.14",
    "prop-types": "^15.8.1",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.3",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.5.0",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@eslint/js": "^9.9.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "canvas": "^3.1.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.45",
    "supabase": "^2.26.9",
    "tailwindcss": "^3.4.10",
    "terser": "^5.39.0",
    "vite": "^5.4.1",
    "vitest": "^3.2.4"
  }
